{
  "name": "flashloan-arbitrage",
  "version": "1.0.0",
  "description": "Cross-chain flashloan arbitrage system with opportunity detection",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "build:contracts": "npx hardhat compile",
    "test": "npx hardhat test",
    "test:etherscan": "node test-etherscan-v2.js",
    "test:rate-limiting": "node test-rate-limiting.js",
    "deploy": "npx hardhat run scripts/deploy.ts",
    "start": "node dist/src/index.js",
    "start:web": "node dist/src/web-server.js",
    "start:dashboard": "node dist/src/dashboard.js",
    "dev": "ts-node src/index.ts",
    "dev:web": "ts-node --esm src/web-server.ts",
    "dev:dashboard": "ts-node --esm src/dashboard.ts",
    "example": "ts-node src/example.ts",
    "etherscan:demo": "ts-node src/etherscan-v2-demo.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "flashloan",
    "arbitrage",
    "defi",
    "cross-chain",
    "ethereum"
  ],
  "author": "Flashloan Arbitrage Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "hardhat": "^2.19.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "ethers": "^6.8.0",
    "express": "^4.21.2",
    "web3": "^4.16.0",
    "ws": "^8.14.0"
  }
}
